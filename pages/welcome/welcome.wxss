}
.container{
  width: 100%;
  height: 100%;
  position: relative;
}
.container .back{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 1;
}
.container .btn,.container button{
  width: 110px;
  height: 30px;
  position: absolute;
  top: 30%;
  left: 50%;
